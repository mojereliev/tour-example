    "babel-eslint": "^10.0.1",
    "bemto.pug": "2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^11.0.0",
